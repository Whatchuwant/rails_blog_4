<h1>Welcome to Blog 4!</h1>

<h1>Hello, Rails!</h1>
<%= link_to 'My Blog', controller: 'articles' %>

<h1>Sign Up</h1>
<%=form_for User.new do |f| %>
<%=f.label :username %>
<%=f.text_field :username %>
<br>
<br>
<%=f.label :password %>
<%=f.password_field :password %>
<br>
<br>

<%=f.submit "Sign Up" %>
<%end%>
